{
  "name": "@captaincodeman/app-metadata",
  "version": "3.0.0-beta.0",
  "description": "Page meta-tags for SEO and social sharing",
  "browser": "app-metadata.min.js",
  "main": "app-metadata.js",
  "module": "index.js",
  "types": "index.d.ts",
  "unpkg": "app-metadata.min.js",
  "scripts": {
    "build:es": "tsc --module es2015 --declaration",
    "build:cjs": "rollup -c",
    "build": "npm run build:es && npm run build:cjs",
    "test": "echo \"Error: no test specified\" && exit 1",
    "mypublish": "npm run build && cp readme.md package.json dist && npm publish dist --tag beta --access=public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CaptainCodeman/app-metadata.git"
  },
  "keywords": [
    "seo",
    "metadata"
  ],
  "author": "simon@captaincodeman.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/CaptainCodeman/app-metadata/issues"
  },
  "homepage": "https://github.com/CaptainCodeman/app-metadata#readme",
  "devDependencies": {
    "rollup": "^0.62.0",
    "rollup-plugin-terser": "^1.0.1",
    "rollup-plugin-typescript": "^0.8.1",
    "typescript": "^2.9.2"
  }
}
